      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="text" value="&lt;Text$NAME$&gt;&#10;    $END$&#10;&lt;/Text&gt;" description="" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>