    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rexport" value="export default class $NAME$ extends Component {&#10;    $END$&#10;}&#10;" description="export default class" toReformat="false" toShortenFQNames="true">
