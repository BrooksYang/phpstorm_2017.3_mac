<templateSet group="React Templates ">
  <template name="rimport-component" value="import React, { Component } from 'react';&#10;import {$NAME$} from 'react-native';&#10;" description="Import React" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="rimport" value="import { $libName$ } from '$libName$';" description="Import React Component" toReformat="false" toShortenFQNames="true">
    <variable name="libName" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="rexport" value="export default class $NAME$ extends Component {&#10;    $END$&#10;}&#10;" description="export default class" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>