    </context>
  </template>
  <template name="view" value="&lt;View$NAME$&gt;&#10;    $END$&#10;&lt;/View&gt;" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
